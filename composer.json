{
    "name": "ivanitch/fix-price-m",
    "description": "ivanitch/fix-price-m",
    "keywords": ["yii2", "api", "fix-price-mini"],
    "type": "project",
    "minimum-stability": "stable",
    "authors": [
        {
            "name": "Sergey Ivanov",
            "email": "amurweb@gmail.com"
        }
    ],
    "require": {
        "php": ">=8.4.0",
        "ext-curl": "*",
        "ext-http": "*",
        "yiisoft/yii2": "~2.0",
        "yiisoft/yii2-redis": "^2.0"
    },
    "require-dev": {
        "symfony/var-dumper": "^7.3"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "yiisoft/yii2-composer": true
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ],
    "autoload": {
        "psr-4": {
            "src\\": "src/"
        }
    }
}
